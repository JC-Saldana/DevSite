<div class="developers">
	{{#each users as |user|}}
	<a href="/user/{{user.id}}" class="developer-container">
		<div class="developer">
			<img src="{{user.avatar}}" alt="img" class="developer-pic">
			<p><strong>{{user.name}}</strong></p>
			<p>{{user.currentJob}}</p>
			<ul class="list-horizontal">
				{{#each user.skills as |skill|}}
				<li>
					<small>{{skill}}</small>
				</li>
				{{/each}}
			</ul>
		</div>
	</a>
	{{/each}}
</div>